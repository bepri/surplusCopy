{
    "name": "Surplus Copier",
    "description": "Extension to copy surplus informtion in the correct format",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [ {
            "matches": ["https://rt.eecs.utk.edu/*"],
            "js": ["content-script.js"]
        } ],
    "permissions": [
        "webNavigation",
        "activeTab"
    ]
}